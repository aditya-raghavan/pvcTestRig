<Page x:Class="UI_TestRig.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:sys="clr-namespace:System;assembly=mscorlib"
      xmlns:local="clr-namespace:UI_TestRig"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainPage" MinHeight="768" MinWidth="1366" FontFamily="Arial" Background="White">

    
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"></RowDefinition>
                <RowDefinition Height="auto"></RowDefinition>
                <RowDefinition Height="auto"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="auto"></RowDefinition>
            </Grid.RowDefinitions>



            <Grid Grid.Row="0" Name="TopGrid" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.7*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="0.5*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <TextBlock x:Name="TitleTextBox" Background="#cccccc" VerticalAlignment="Center" Padding="0,16" Grid.Column="0" TextAlignment="Center" Text="PhotoVoltaic Junction Box Diode Test Rig" FontWeight="Bold" FontSize="21"></TextBlock>
                <StackPanel Name="UserStack" Grid.Column="1">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="3*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBlock x:Name="userTextBlock" TextAlignment="Right" Text="User" FontWeight="Bold" FontSize="18" Padding="2" Background="#999999" ></TextBlock>
                        </Border>
                        <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBox x:Name="userTextBox" Grid.Column="1" Padding="3"></TextBox>
                        </Border>

                    </Grid>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="80.5"></ColumnDefinition>
                            <ColumnDefinition Width="3*"></ColumnDefinition>
                            <ColumnDefinition Width="0.5*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>

                        </Grid.ColumnDefinitions>


                        <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBlock x:Name="programTextBlock" TextAlignment="Right" Text="Program" FontWeight="Bold" FontSize="18" Padding="2" Background="#999999" ></TextBlock>
                        </Border>
                        <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBox x:Name="programTextBox" Grid.Column="1" Padding="3"></TextBox>
                        </Border>
                        <Border Grid.Column="2" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBlock x:Name="nrTextBlock" TextAlignment="Right" Text="Nr" FontWeight="Bold" FontSize="18" Padding="2" Background="#999999" ></TextBlock>
                        </Border>
                        <Border Grid.Column="4" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBox x:Name="nrTextBox" Grid.Column="1" Padding="3"></TextBox>
                        </Border>

                    </Grid>
                </StackPanel>


                <StackPanel Name="ModeStack" Grid.Column="2">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="3*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBlock x:Name="modeTextBlock" TextAlignment="Right" Text="Mode" FontWeight="Bold" FontSize="18" Padding="2" Background="#999999" ></TextBlock>
                        </Border>
                        <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBlock x:Name="autoTextBlock" Text="AUTO" TextAlignment="Center" Padding="2" FontSize="18" FontWeight="Bold" Background="#66ffff"></TextBlock>
                        </Border>

                    </Grid>

                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="3*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>


                        <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBlock x:Name="statusTextBlock" TextAlignment="Right" Text="Status" FontWeight="Bold" FontSize="18" Padding="2" Background="#999999" ></TextBlock>
                        </Border>
                        <Border x:Name="autoRunningTextBlock" Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                            <TextBlock x:Name="autoRunningTextBlock1" Text="AUTO RUNNING" Foreground="Red" TextAlignment="Center" Padding="2" FontSize="18" FontWeight="Bold" ></TextBlock>
                        </Border>

                    </Grid>

                </StackPanel>

                <StackPanel Grid.Column="3">
                    <Border  BorderThickness="2" BorderBrush="#4d4d4d">
                        <TextBlock x:Name="dateTextBlock" Text="{Binding Source={x:Static sys:DateTime.Today},StringFormat='{}{0:dd-MM-yyyy}'}" Foreground="White" Background="Black" TextAlignment="Center" Padding="2" FontSize="18" FontWeight="Bold" ></TextBlock>
                    </Border>
                    <Border BorderThickness="2" BorderBrush="#4d4d4d">
                        <TextBlock x:Name="timeTextBlock" Text="{Binding Source={x:Static sys:DateTime.Now},
                   StringFormat='{}{0: hh:mm:ss}'}" Foreground="White" Background="Black" TextAlignment="Center" Padding="2" FontSize="18" FontWeight="Bold" ></TextBlock>
                    </Border>


                </StackPanel>


            </Grid>

            <Border  BorderThickness="2" BorderBrush="#4d4d4d" Grid.Row="1">
                <TextBlock x:Name="mainScreenTextBlock" Text="Main Screen" Padding="5" FontSize="14" FontWeight="Bold" Foreground="white" Background="#ff6600"></TextBlock>
            </Border>

            <Border Name="MainContentBorder" Padding="5" Grid.Row="2">
                <Grid Margin="0,0,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="10*"></ColumnDefinition>
                        <ColumnDefinition Width="16*"></ColumnDefinition>
                        <ColumnDefinition Width="10*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Border Padding="2" BorderThickness="2" Grid.Column="0" >
                        <Border.BorderBrush>
                            <VisualBrush>
                                <VisualBrush.Visual>
                                    <Rectangle StrokeDashArray="4 2" Stroke="Gray" StrokeThickness="1" Width="{Binding RelativeSource={RelativeSource AncestorType={x:Type Border}}, Path=ActualWidth}" Height="{Binding RelativeSource={RelativeSource AncestorType={x:Type Border}}, Path=ActualHeight}"/>
                                </VisualBrush.Visual>
                            </VisualBrush>
                        </Border.BorderBrush>



                        <StackPanel>


                            <TextBlock x:Name="testInformationTextBlock" Text="Test Information" Padding="10" FontSize="14" TextAlignment="Center" FontWeight="Bold" Foreground="white" Background="#ff8533"></TextBlock>
                            <StackPanel Background="#cccccc">
                                <ComboBox x:Name="modelComboBox" Width="300" Height="30"  HorizontalAlignment="Center" Margin="10,10,10,0"  >
                                    <ComboBoxItem IsSelected="True" Content="TREY 1256"></ComboBoxItem>
                                    <ComboBoxItem Content="TREY 1256"></ComboBoxItem>
                                    <ComboBoxItem Content="TREY 1256"></ComboBoxItem>
                                    <ComboBoxItem Content="TREY 1256"></ComboBoxItem>
                                    <ComboBoxItem Content="TREY 1256"></ComboBoxItem>
                                    <ComboBoxItem Content="TREY 1256"></ComboBoxItem>


                                </ComboBox>

                                <TextBlock x:Name="comparisonCompletedTextBlock" Margin="10,30,0,10" Text="Comparison Completed" Foreground="Red"/>

                            </StackPanel>

                            <Button x:Name="selectModelButton" Height="50" Content="Select Model" FontSize="18" FontWeight="Bold" Foreground="white" Background="#999999" Margin="0,3" >
                                <Button.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="CornerRadius" Value="7"/>
                                    </Style>
                                </Button.Resources>
                            </Button>

                            <StackPanel>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="2*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBlock x:Name="operatorTextBlock" TextAlignment="Right" Text="Operator" FontWeight="Bold" FontSize="14" Padding="2" Background="#999999" ></TextBlock>
                                    </Border>
                                    <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBox x:Name="operatorTextBox" Grid.Column="1" Padding="3"></TextBox>
                                    </Border>



                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="2*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBlock x:Name="diodeCodeTextBlock" TextAlignment="Right" Text="Diode Code" FontWeight="Bold" FontSize="14" Padding="2" Background="#999999" ></TextBlock>
                                    </Border>
                                    <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBox x:Name="diodecodeTextBox" Grid.Column="1" Padding="3"></TextBox>
                                    </Border>



                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="2*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBlock x:Name="customerCodeTextBlock" TextAlignment="Right" Text="Customer Code" FontWeight="Bold" FontSize="14" Padding="2" Background="#999999" ></TextBlock>
                                    </Border>
                                    <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBox x:Name="customercodeTextBox" Grid.Column="1" Padding="3"></TextBox>
                                    </Border>



                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="2*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBlock x:Name="additionalCodeTextBlock" TextAlignment="Right" Text="Additional Code" FontWeight="Bold" FontSize="14" Padding="2" Background="#999999" ></TextBlock>
                                    </Border>
                                    <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBox x:Name="additionalcodeTextBox" Grid.Column="1" Padding="3"></TextBox>
                                    </Border>



                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="2*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBlock x:Name="jbserialNoTextBlock" TextAlignment="Right" Text="JB Serial Nr" FontWeight="Bold" FontSize="14" Padding="2" Background="#999999" ></TextBlock>
                                    </Border>
                                    <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBox x:Name="jbserialTextBox" Grid.Column="1" Padding="3"></TextBox>
                                    </Border>



                                </Grid>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="2*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBlock x:Name="batchNumberTextBlock" TextAlignment="Right" Text="Batch Number" FontWeight="Bold" FontSize="14" Padding="2" Background="#999999" ></TextBlock>
                                    </Border>
                                    <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                        <TextBox x:Name="batchnoTextBox" Grid.Column="1" Padding="3"></TextBox>
                                    </Border>



                                </Grid>

                            </StackPanel>

                            <TextBlock Height="50"></TextBlock>
                            <Border BorderThickness="2" BorderBrush="#4d4d4d" Margin="0,0,0,5">
                                <TextBlock x:Name="modeMainTextBlock" Text="Mode" Padding="7" FontSize="14" TextAlignment="Center" FontWeight="Bold" Foreground="white" Background="#ff8533" ></TextBlock>
                            </Border>
                            <Border  BorderThickness="2" BorderBrush="#4d4d4d">
                                <TextBlock x:Name="autoMainTextBlock" Text="Auto" Padding="7" FontSize="14" TextAlignment="Center" FontWeight="Bold" Foreground="white" Background="#99ffcc"></TextBlock>
                            </Border>


                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>

                                <Button x:Name="manualButton" Grid.Column="0" Height="50" Content="Manual" FontSize="18" FontWeight="Bold" Foreground="white" Background="#999999" Margin="3,3" >
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="7"/>
                                        </Style>
                                    </Button.Resources>
                                </Button>

                                <Button x:Name="autoButton" Grid.Column="1" Height="50" Content="Auto" FontSize="18" FontWeight="Bold" Foreground="white" Background="#999999" Margin="3,3,0,3" HorizontalAlignment="Left" Width="171" >
                                    <Button.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="7"/>
                                        </Style>
                                    </Button.Resources>
                                </Button>


                            </Grid>



                        </StackPanel>
                    </Border>

                    <Border Padding="2" BorderThickness="2" Grid.Column="2" >
                        <Border.BorderBrush>
                            <VisualBrush>
                                <VisualBrush.Visual>
                                    <Rectangle StrokeDashArray="4 2" Stroke="Gray" StrokeThickness="1" Width="{Binding RelativeSource={RelativeSource AncestorType={x:Type Border}}, Path=ActualWidth}" Height="{Binding RelativeSource={RelativeSource AncestorType={x:Type Border}}, Path=ActualHeight}"/>
                                </VisualBrush.Visual>
                            </VisualBrush>
                        </Border.BorderBrush>

                        <StackPanel>
                            <TextBlock x:Name="testDataTextBlock" Text="Test Data" Padding="10" FontSize="14" TextAlignment="Center" FontWeight="Bold" Foreground="white" Background="#ff8533" Margin="0,0,0,7"></TextBlock>

                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1.3*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="1.1*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Column="0" Grid.Row="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="itemTextBlock" TextAlignment="Center" Text="Item" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="1" Grid.Row="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="unitsTextBlock" TextAlignment="Center" Text="Units" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="2" Grid.Row="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="lowTextBlock" TextAlignment="Center" Text="Low" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="3" Grid.Row="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="actualTestDataTextBlock" TextAlignment="Center" Text="Actual" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="4" Grid.Row="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="highTextBlock" TextAlignment="Center" Text="High" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="5" Grid.Row="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="statusTestDataTextBlock" TextAlignment="Center" Text="Status" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>

                                <Border Grid.Column="0" Grid.Row="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdCurrentTextBlock" TextAlignment="Center" Text="Fwd Current" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5"  />
                                </Border>
                                <Border Grid.Column="1" Grid.Row="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdCurrentUnitsTextBlock" TextAlignment="Center" Text="Amps" FontWeight="Bold" FontSize="14" Padding="2"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="2" Grid.Row="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdCurrentLowTextBlock" TextAlignment="Center" Text="+6.0" FontWeight="Bold" FontSize="14" Padding="2"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="3" Grid.Row="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdCurrentActualTextBlock" TextAlignment="Center" Text="+0.0" FontWeight="Bold" FontSize="14" Padding="2" Foreground="Blue"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="4" Grid.Row="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdCurrentHighTextBlock" TextAlignment="Center" Text="+6.3" FontWeight="Bold" FontSize="14" Padding="2"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="5" Grid.Row="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdCurrentStatusTextBlock" TextAlignment="Center" Text="PENDNG" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>

                                <Border Grid.Column="0" Grid.Row="2" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdVoltageTextBlock" TextAlignment="Center" Text="Fwd Voltage" FontWeight="Bold" Padding="2" Background="#cccccc" Margin="0.4,0.4,0,0" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="1" Grid.Row="2" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdVoltageUnitTextBlock" TextAlignment="Center" Text="Volts" FontWeight="Bold" FontSize="14" Padding="2"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="2" Grid.Row="2" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdVoltageLowTextBlock" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="2"  ><Run Text="+"/><Run Text="1.3"/></TextBlock>
                                </Border>
                                <Border Grid.Column="3" Grid.Row="2" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdVoltageActualTextBlock" TextAlignment="Center" Text="+0.0" FontWeight="Bold" FontSize="14" Padding="2" Foreground="Blue"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="4" Grid.Row="2" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdVoltageHighTextBlock" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="2"  ><Run Text="+"/><Run Text="2.7"/></TextBlock>
                                </Border>
                                <Border Grid.Column="5" Grid.Row="2" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdVoltageStatusTextBlock" TextAlignment="Center" Text="PENDNG" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>

                                <Border Grid.Column="0" Grid.Row="3" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop1TextBlock" TextAlignment="Center" FontWeight="Bold" Padding="2" Background="#cccccc" Text="Fwd Drop 1" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="1" Grid.Row="3" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop1UnitTextBlock" TextAlignment="Center" Text="mV" FontWeight="Bold" FontSize="14" Padding="2"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="2" Grid.Row="3" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop1LowTextBlock1" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="2"  ><Run Text="+"/><Run Text="300"/></TextBlock>
                                </Border>
                                <Border Grid.Column="3" Grid.Row="3" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop1ActualTextBlock" TextAlignment="Center" Text="+0" FontWeight="Bold" FontSize="14" Padding="2" Foreground="Blue"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="4" Grid.Row="3" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop1HighTextBlock" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="2"  ><Run Text="+"/><Run Text="500"/></TextBlock>
                                </Border>
                                <Border Grid.Column="5" Grid.Row="3" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop1StatusTextBlock" TextAlignment="Center" Text="PENDNG" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>

                                <Border Grid.Column="0" Grid.Row="4" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop2TextBlock" TextAlignment="Center" Text="Fwd Drop 2" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="1" Grid.Row="4" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop2UnitsTextBlock" TextAlignment="Center" Text="mV" FontWeight="Bold" FontSize="14" Padding="2"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="2" Grid.Row="4" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop2LowTextBlock" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="2"  ><Run Text="+"/><Run Text="300"/></TextBlock>
                                </Border>
                                <Border Grid.Column="3" Grid.Row="4" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop2ActualTextBlock" TextAlignment="Center" Text="+0" FontWeight="Bold" FontSize="14" Padding="2" Foreground="Blue"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="4" Grid.Row="4" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop2HighTextBlock" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="2"  ><Run Text="+"/><Run Text="500"/></TextBlock>
                                </Border>
                                <Border Grid.Column="5" Grid.Row="4" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop2StatusTextBlock" TextAlignment="Center" Text="PENDNG" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>

                                <Border Grid.Column="0" Grid.Row="5" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop3TextBlock" TextAlignment="Center" Text="Fwd Drop 3" FontWeight="Bold" Padding="2" Background="#cccccc" FontSize="11.5" />
                                </Border>
                                <Border Grid.Column="1" Grid.Row="5" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop3UnitsTextBlock" TextAlignment="Center" Text="mV" FontWeight="Bold" FontSize="14" Padding="2"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="2" Grid.Row="5" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop3LowTextBlock" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="2"  ><Run Text="+"/><Run Text="300"/></TextBlock>
                                </Border>
                                <Border Grid.Column="3" Grid.Row="5" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop3ActualTextBlock" TextAlignment="Center" Text="+0" FontWeight="Bold" FontSize="14" Padding="2" Foreground="Blue"  ></TextBlock>
                                </Border>
                                <Border Grid.Column="4" Grid.Row="5" BorderThickness="2" BorderBrush="#4d4d4d">
                                    <TextBlock x:Name="fwdDrop3HighTextBlock" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="2"  ><Run Text="+"/><Run Text="500"/></TextBlock>
                                </Border>
