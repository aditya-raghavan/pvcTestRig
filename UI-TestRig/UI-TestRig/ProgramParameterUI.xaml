                    </Border>
                </Grid>
                <Grid >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="0.4*"></ColumnDefinition>
                        <ColumnDefinition Width="0.3*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                        <TextBlock x:Name="negativeTolTextBlock"  Text="Negative Tol" Foreground="Black" VerticalAlignment="Bottom" TextAlignment="Center" Padding="2" FontSize="12" FontWeight="Bold" Margin="0.4,0,-0.2,3.2" ></TextBlock>
