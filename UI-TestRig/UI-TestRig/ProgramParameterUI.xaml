                            </Border>

                        </Grid>

                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>


                            <Border Grid.Column="0" BorderThickness="2" BorderBrush="#4d4d4d">
                                <TextBlock x:Name="barCodePrinterTextBlock"  TextAlignment="Right" Text="Bar Code Printer" FontWeight="Bold" FontSize="12" Padding="2" Background="White" ></TextBlock>
                            </Border>
                            <Border Grid.Column="1" BorderThickness="2" BorderBrush="#4d4d4d">
                                <ComboBox x:Name="barCodePrinterCombo" FontSize="12" FontWeight="Bold" Padding="2" Background="#ccffff" HorizontalContentAlignment="Center">
                                    
                                </ComboBox>
                            </Border>

                        </Grid>

